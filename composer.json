{
  "name": "lxlxw/bytom-php-sdk",
  "description": "PHP SDK to interact with Bytomd",
  "keywords": [
    "bytom",
    "php",
    "sdk"
  ],
  "type": "library",
  "homepage": "https://github.com/lxlxw/bytom-php-sdk",
  "license": "Apache License Version 2.0",
  "authors": [
    {
      "name": "lxlxw",
      "email": "linxiaowu@xwlin.com"
    }
  ],
  "require": {
    "php": ">=5.4"
  },
  "require-dev": {
    "phpunit/phpunit": "^4.8.24",
    "phpmd/phpmd": "~2.4",
    "squizlabs/php_codesniffer": "~2.6",
    "apigen/apigen": "~4.1",
    "indigophp/hash-compat": "~1.1.0"
  },
  "autoload": {
    "psr-4": {
      "Bytom\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Bytom\\Tests\\": "tests/"
    }
  }
}
